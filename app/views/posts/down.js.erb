$('#<%=@post.user.username.gsub(/ /, "-") rescue "Vigap"%>-post-upvote').text(" " + <%=@post.cached_votes_up%>);
$('#<%=@post.user.username.gsub(/ /, "-") rescue "Vigap"%>-post-downvote').text(" " + <%=@post.cached_votes_down%>);

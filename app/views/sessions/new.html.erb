<h1>Log IN</h1>

<%= form_tag :sessions do %>
    <div class="field">
      <%= label_tag :email %>
      <%= text_field_tag :email %>
    </div>
    <div class="field">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div class="action">
      <%= submit_tag 'Log IN' %>
    </div>
<% end %>
